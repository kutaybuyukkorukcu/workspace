package com.kutay.github.projeleri.verone;

public class P11_CokKolonluDongu {

	public static void main(String[] args) {
		int[] colArray1 = new int[6];
		int[] colArray2 = new int[6];
		int[] colArray3 = new int[6];
		int[] colArray4 = new int[6];

		for (int satir = 0; satir < 6; satir++) {

			for (int i = 0; i < 99; i++) {
				if ((i + 1) % 15 == 0) {
					colArray1[satir] = (i + 1);
				}
			}

			for (int x = 0; x < 30; x++) {
				if ((x + 1) % 5 == 0) {
					colArray2[satir] = (x + 1);
				}
			}

			for (int y = 100; 100 > 49; y--) {
				if ((y) % 10 == 0) {
					colArray3[satir] = y;
				}
			}

			for (int z = 0; z < 6; z++) {
				colArray4[satir] = (int) Math.pow(z + 1, 2);
			}

		}

		for (int a = 0; a < 6; a++) {
			System.out.print(colArray1[a] + colArray2[a] + colArray3[a] + colArray4[a]);
		}
	}
}

package com.kutay.github.projeleri.verone;

public class P11_CokKolonluDongu {

	public static void main(String[] args) {
		for (int row = 0; row < 6; row++) {

			for (int i = 0; i < 99; i++) {
				if ((i + 1) % 15 == 0) {
				int[] col1Array;
				col1Array[row] = (i + 1)l	//System.out.print(i + 1 + " ");
					break;
				}
			}

			for (int x = 0; x < 30; x++) {
				if ((x + 1) % 5 == 0) {
					System.out.print(x + 1 + " ");
					break;
				}
			}

			for (int y = 100; 100 > 49; y--) {
				if ((y) % 10 == 0) {
					System.out.print(y + " 1");
					break;
				}
			}

			for (int z = 0; z < 6; z++) {
				System.out.print(Math.pow(z + 1, 2) + "\n");
				break;
			}

		}
	}
}
